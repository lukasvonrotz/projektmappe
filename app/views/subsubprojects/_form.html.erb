    <div class="field form-group">
      <%= subsubproject.label :hourrate_parametrierung, 'Stundenansatz Parametrierung' %><br>
      <%= subsubproject.text_field :hourrate_parametrierung, :class => "form-control" %>
    </div>
    <div class="field form-group">
      <%= subsubproject.label :complexity_parametrierung, 'Komplexitätsfaktor Parametrierung' %><br>
