      <%= subsubproject.text_field :complexity_konformitaet, :class => "form-control" %>
    </div>

    <%= subsubproject.hidden_field :subproject_id, :value => subprojectid %>

    <div class="actions form-group">
      <% if context %>
          <%= subsubproject.submit 'Erstellen', class: "btn btn-success", data: { disable_with: "Verarbeite..." }  %>
      <% else %>
          <%= subsubproject.submit 'Update', class: "btn btn-success", data: { disable_with: "Verarbeite..." }  %>
      <% end %>
    </div>

<% end %>
