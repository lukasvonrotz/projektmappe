<h1>IO-Gruppe editieren</h1>
<% if @iogroup.errors.any? %>
    <div class="alert alert-danger">
      <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
      <%= @iogroup.errors.full_messages.first %>
    </div>
<% end %>

<%= render 'form', :iogroup => @iogroup, :iotypeid => @iogroup.iotype.id,
           :switchgearcombinationid => @iogroup.switchgearcombination.id, :context => false  %>
<%= link_to "Zurück zu IO-Gruppen-Übersicht", iogroups_path %>