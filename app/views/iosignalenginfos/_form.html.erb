<%= form_for [iosignal, iosignalenginfo] do |iosignalenginfo| %>
    <td>
      <a class="btn btn-sm btn-info iosignalenginfo_edit iosignalenginfo<%=iosignalenginfoid.to_s%>_edit"
         onclick="editLine(<%=iosignalenginfoid%>);">Edit</a>
    </td>
    <td>
      <%= iosignal_tagname %></br>
    </td>
    <td>
      <%= iosignalenginfo.text_field :pendenz,
                                   :class => "form-control iosignalenginfo_fld iosignalenginfo" + iosignalenginfoid.to_s + "_fld" %>
    </td>
    <td>
      <% if !pendenzdate.nil? %>
        <%= iosignalenginfo.text_field :pendenz_datum,
              :value => pendenzdate.try(:strftime,'%d/%m/%Y %H:%M'),
              :class => "form-control iosignalenginfo_fld iosignalenginfo" + iosignalenginfoid.to_s + "_fld" %>
      <% else %>
          <%= iosignalenginfo.text_field :pendenz_datum,
                                         :class => "form-control iosignalenginfo_fld iosignalenginfo" + iosignalenginfoid.to_s + "_fld pendenz" + iosignalenginfoid.to_s + "_date" %>
      <% end %>
    </td>
    <td>
      <%= iosignalenginfo.text_field :pendenz_wer,
                                     :id => "pendenz" + iosignalenginfoid.to_s,
                                     :class => "form-control iosignalenginfo_fld iosignalenginfo" + iosignalenginfoid.to_s + "_fld inputdetection" %>
    </td>
    <td>
      <%= iosignalenginfo.text_field :pendenz_antwort,
                                     :class => "form-control iosignalenginfo_fld iosignalenginfo" + iosignalenginfoid.to_s + "_fld" %>
    </td>
    <td>
      <% if !pendenzantwortdate.nil? %>
          <%= iosignalenginfo.text_field :pendenz_antwort_datum,
                                         :value => pendenzantwortdate.try(:strftime,'%d/%m/%Y %H:%M'),
