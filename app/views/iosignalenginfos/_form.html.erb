      <% end %>
    </td>
    <td>
      <%= iosignalenginfo.text_field :pendenz_antwort_wer,
                                     :id => "pendenz_antwort" + iosignalenginfoid.to_s,
                                     :class => "form-control iosignalenginfo_fld iosignalenginfo" + iosignalenginfoid.to_s + "_fld inputdetection" %>
    </td>
    <td>
      <%= iosignalenginfo.text_field :frage,
